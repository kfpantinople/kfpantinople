<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.example.joxxxx.footscapes.JournalUI">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginTop="11dp"
            app:srcCompat="@drawable/paperplane" />

        <TextView
            android:id="@+id/textView67"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView19"
            android:layout_marginBottom="27dp"
            android:layout_marginStart="34dp"
            android:layout_toEndOf="@+id/imageView19"
            android:text="JOURNAL"
            android:textSize="35dp" />

        <TextView
            android:id="@+id/textView68"
            android:layout_width="300dp"
            android:layout_height="250dp"
            android:text="Enter text here..."
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <Button
            android:id="@+id/button14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView67"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="33dp"
            android:onClick="buttonclick"
            android:text="SUBMIT" />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
